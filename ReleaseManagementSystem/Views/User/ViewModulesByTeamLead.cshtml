@model IEnumerable<ReleaseManagementSystem.Models.ViewModulesbyTeamLeader>

@{
    ViewBag.Title = "ViewModulesByTeamLead";
}

<h2>ViewModulesByTeamLead</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProjectId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Module_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Assigned_Dev_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Assigned_Tester_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status_Dev)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status_Tester)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProjectId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Module_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Assigned_Dev_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Assigned_Tester_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status_Dev)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status_Tester)
        </td>
        <td>
            @Html.ActionLink("Approve Status", "ApproveModulesByTeamLead", new { moduleId = item.Module_Name })

        </td>
    </tr>
}

</table>
