@model IEnumerable<ReleaseManagementSystem.Models.Tester>

@{
    ViewBag.Title = "ViewTesterProjects";
}

<h2>ViewTesterProjects</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Project_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Module_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Assign_Developer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dev_status)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Project_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Module_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Assign_Developer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dev_status)
            </td>
            <td>
                @Html.ActionLink("Raise Bug", "ViewTesterProjects", new { tlid = item.Module_Name, pId = item.Project_Id })

            </td>
            <td>
                @Html.ActionLink("Update Status", "UpdateTesterStatus", new { tlid = item.Module_Name, pId = item.Project_Id })

            </td>
            <td>
                @Html.ActionLink("View Bugs", "ViewTesterProjects", new { tlid = item.Module_Name, pId = item.Project_Id })

            </td>
        </tr>
    }

</table>

