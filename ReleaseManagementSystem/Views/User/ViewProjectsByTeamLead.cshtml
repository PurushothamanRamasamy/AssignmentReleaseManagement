            @Html.DisplayFor(modelItem => item.Project_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProjectName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExpectedStartDate)
